{
  "name": "mondo-app",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "lint": "better-npm-run lint",
    "prebuild": "rm -rf dist",
    "build": "better-npm-run build",
    "prestart": "ls -l",
    "start": "better-npm-run start",
    "prestart:dev": "better-npm-run prestart:dev",
    "start:dev": "better-npm-run start:dev",
    "test:quick": "better-npm-run test:quick",
    "pretest": "npm run lint",
    "test": "better-npm-run test"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "node_modules"
    ]
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint --ext .jsx,.js src/** webpack/** index.js webpack.config.babel.js",
      "env": {
        "NODE_PATH": "./:./src:./src/common"
      }
    },
    "start": {
      "command": "node index.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./:./src:./src/common"
      }
    },
    "prestart:dev": {
      "command": "webpack -p --progress",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command": "nodemon --watch src",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./:./src:./src/common"
      }
    },
    "test:quick": {
      "command": "babel-tape-runner src/**/*.spec.js src/**/**/*.spec.js src/**/**/**/*.spec.js | tap-spec",
      "env": {
        "baseUrl": "http://www.example.com/",
        "NODE_ENV": "test",
        "NODE_PATH": "./:./src:./src/common"
      }
    },
    "test": {
      "command": "babel-tap src/*/*.spec.js src/*/*/*.spec.js src/*/*/*/*.spec.js -b --cov --coverage-report=lcov",
      "env": {
        "baseUrl": "http://www.example.com/",
        "NODE_ENV": "test",
        "NODE_PATH": "./:./src:./src/common"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukeSheard/Mondo-Dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LukeSheard/Mondo-Dashboard/issues"
  },
  "homepage": "https://github.com/LukeSheard/Mondo-Dashboard#readme",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "better-npm-run": "0.0.11",
    "bluebird": "^3.4.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.24.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.11.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.2",
    "react-bootstrap-date-picker": "^3.3.1",
    "react-datepicker": "^0.29.0",
    "react-datetime": "^2.5.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.5",
    "react-sticky": "^5.0.4",
    "react-typography": "^0.12.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.11.0",
    "redux-auth-wrapper": "^0.7.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^6.0.1",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "typography": "^0.13.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-isomorphic-tools": "^2.5.7",
    "webpack-multi-configurator": "^1.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-tap": "^5.0.0",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.1",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "jsonwebtoken": "^7.1.9",
    "nodemon": "^1.10.0",
    "react-hot-loader": "^1.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
